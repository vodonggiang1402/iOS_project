//mũi giảm
"sc2tog_crochet_name" = "Single crochet 2 stitches together sc2tog";
"sc2tog_crochet_description" = "";
"sc2tog_crochet_content_1" = "Insert hook into the both loops of the prev row as shown and yarn over hook.";
"sc2tog_crochet_content_2" = "Draw through. Repeat by inserting hook into next st and yarn over hook and draw through.";
"sc2tog_crochet_content_3" = "This makes two incomplete scs. Yarn over and draw through 3 loops on hook all at once.";
"sc2tog_crochet_content_4" = "This completes a sc2tog";

"hdc2tog_crochet_name" = "half-double crochet 2 stitches together hdc2tog";
"hdc2tog_crochet_description" = "";
"hdc2tog_crochet_content_1" = "Yarn over and insert hook into the back loop of chain st.";
"hdc2tog_crochet_content_2" = "Draw through. Yarn over hook and insert hook into the back loop of the next st and draw through.";
"hdc2tog_crochet_content_3" = "This makes two incomplete hdcs. Yarn over and draw through 5 loops on hook all at once.";
"hdc2tog_crochet_content_4" = "This completes a hdc2tog.";

"dc2tog_crochet_name" = "double crochet 2 stitches together dc2tog";
"dc2tog_crochet_description" = "";
"dc2tog_crochet_content_1" = "Insert hook into the back loop of chain st and work 1 incomplete dc. Yarn over hook and insert hook into back loop of the next st and draw through.";
"dc2tog_crochet_content_2" = "Yarn over hook again and draw through first 2 loops on hook.";
"dc2tog_crochet_content_3" = "This makes two incomplete double crochet sts. Yarn over and draw through all 3 loops on hook at once.";
"dc2tog_crochet_content_4" = "This completes a dc2tog.";

"sc3tog_crochet_name" = "Single crochet 3 stitches together sc3tog";
"sc3tog_crochet_description" = "";
"sc3tog_crochet_content_1" = "Insert hook into the both loops of the prev row as shown.";
"sc3tog_crochet_content_2" = "Repeat the previous step for the next two sts.";
"sc3tog_crochet_content_3" = "This makes three incomplete scs. Yarn over and draw through all 4 loops on hook at once.";
"sc3tog_crochet_content_4" = "This completes a sc3tog.";

"sc3tog_skipping_the_center_stich_crochet_name" = "sc3tog(skipping the center st)";
"sc3tog_skipping_the_center_stich_crochet_description" = "";
"sc3tog_skipping_the_center_stich_crochet_content_1" = "Insert hook into the both loops of the first of three sts";
"sc3tog_skipping_the_center_stich_crochet_content_2" = "This makes two incomplete scs. Yarn over and draw through all 3 loops on hook at once.";
"sc3tog_skipping_the_center_stich_crochet_content_3" = "This completes a sc3tog.";

"hdc3tog_crochet_name" = "hdc3tog";
"hdc3tog_crochet_description" = "";
"hdc3tog_crochet_content_1" = "Yarn over and insert hook into the back loop of chain st.";
"hdc3tog_crochet_content_2" = "Work an incomplete hdc. Work 2 more incomplete hdc in each of the next two sts.";
"hdc3tog_crochet_content_3" = "Yarn over hook and draw through all 7 loops on hook at once.";
"hdc3tog_crochet_content_4" = "This completes a hdc3tog";


"dc3tog_crochet_name" = "dc3tog";
"dc3tog_crochet_description" = "";
"dc3tog_crochet_content_1" = "Yarn over and insert hook into the back loop of chain st and work an incomplete dc.";
"dc3tog_crochet_content_2" = "Work 2 more incomplete dc in each of the next 2 sts.";
"dc3tog_crochet_content_3" = "Yarn over hook and draw through all 4 loops on hook at once.";
"dc3tog_crochet_content_4" = "This completes a dc3tog";

